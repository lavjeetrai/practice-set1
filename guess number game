//IN JAVASCRIPT

let num=prompt("ENTER A NUMBER TO START THE GAME : ");
const a=25;
while(num!=a){
    let num2=prompt("WRONG,ENTER A NUMBER AGAIN ");
    num=num2;
}
console.log("YOU WON");
